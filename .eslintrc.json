{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "comma-dangle": ["error", "always-multiline"],
    "@next/next/no-img-element": "off",
    "react/jsx-tag-spacing": ["error", { "beforeSelfClosing": "always" }]
  }
}
